@model List<Product>

@section Footer {
  <div>
    Only for Index footer
  </div>
}

<table class="table">
  <thead>
    <tr>
      <th>Product Name</th>
      <th>Price</th>
    </tr>
  </thead>
  <tbody>
    @foreach (Product item in Model)
    {
      <partial name="_ProductCard" model="@item"/>
    }
  </tbody>
</table>

